{
  "name": "oposs/silverstripe-hcaptcha",
  "type": "silverstripe-vendormodule",
  "description": "Protect your forms with hCaptcha",
  "homepage": "https://oetiker.ch",
  "license": "MIT",
  "keywords": [
    "silverstripe",
    "Spamprotection",
    "captcha",
    "hcaptcha"
  ],
  "support": {
    "issues": "https://github.com/oposs/silverstripe-hcaptcha/issues"
  },
  "authors": [
    {"name": "Tobias Bossert", "email": "bossert@oetiker.ch"}
  ],
  "require": {
    "silverstripe/cms": "^5.0",
    "silverstripe/framework": "^5.0",
    "silverstripe/spamprotection": "^4.0",
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Oposs\\hCaptcha\\": "src/",
      "Oposs\\hCaptcha\\Forms\\" : "src/Forms"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
